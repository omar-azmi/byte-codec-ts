{
  "module": "./esm/mod.js",
  "types": "./types/mod.d.ts",
  "name": "byte_codec_ts",
  "version": "0.2.0",
  "description": "a schema based serializer for primitive javascript types with no external dependencies",
  "author": "Omar Azmi",
  "license": "Lulz plz don't steal yet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omar-azmi/byte_codec_ts.git"
  },
  "bugs": {
    "url": "https://github.com/omar-azmi/byte_codec_ts/issues"
  },
  "devDependencies": {
    "@types/node": "16.11.37",
    "typescript": "^4.9.3",
    "typedoc": "^0.23.21",
    "esbuild": "^0.15.14"
  },
  "keywords": [
    "serializer",
    "schema",
    "binary",
    "codec",
    "packing",
    "parser"
  ],
  "scripts": {
    "build-docs": "npx typedoc",
    "build-dist": "npm run build-esm && npm run build-esm-minify && npm run build-iife && npm run build-iife-minify",
    "build-esm": "npx esbuild \"./src/mod.ts\" --bundle --format=esm --outfile=\"./dist/byte_codec_ts.esm.js\"",
    "build-esm-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=esm --outfile=\"./dist/byte_codec_ts.esm.min.js\"",
    "build-iife": "npx esbuild \"./src/mod.ts\" --bundle --format=iife --outfile=\"./dist/byte_codec_ts.iife.js\"",
    "build-iife-minify": "npx esbuild \"./src/mod.ts\" --bundle --minify --format=iife --outfile=\"./dist/byte_codec_ts.iife.min.js\""
  },
  "exports": {
    ".": {
      "import": {
        "default": "./esm/mod.js",
        "types": "./types/mod.d.ts"
      }
    }
  }
}